{
  "expo": {
    "name": "pizza-order-expo",
    "slug": "pizza-order-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pizza.png",
    "scheme": "pizzaorderexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/pizza.png",
        "backgroundImage": "./assets/images/pizza.png",
        "monochromeImage": "./assets/images/pizza.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kymsh2.pizzaorderexpo"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/pizza.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pizza.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "acfc8b01-f31e-4869-9535-3c4083dfc434"
      }
    }
  }
}
